<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="../stylesheets/allUsers.css" />
</head>

<body>
    <div id="main">
        <div id="left">
            <div class="logo" style="
            display: flex;
            justify-content: center;
            width: 20vw;
            height: 5vw;
            background-color: red;
          ">
                <img style="width: 18%" src="../images/apple-touch-icon.png" alt="" />
            </div>
            <div class="features">
                <a href="/allOrders">
                    <div class="opt">All Orders</div>
                </a>
                <a href="/addProductPage">
                    <div class="opt">Add Products</div>
                </a>
                <a href="/allProducts">
                    <div class="opt">All Products</div>
                </a>
                <a href="/advertiseOffer">
                    <div class="opt">Advertise New Offer</div>
                </a>
                <a href="/allUsers">
                    <div class="opt">All Users</div>
                </a>
                <a href="/totalRevenue">
                    <div class="opt">Total Revenue</div>
                </a>
            </div>
        </div>
        <div id="right">
            <marquee behavior="scroll" direction="left">
                <img style="max-height: 40px;" src="../images/apple-touch-icon.png" alt="">
            </marquee>

            <h4>Total Users - <%= users.length %> </h4>
            
            <% users.forEach(user => {%>
                <h1><%= user.username %> </h1>  
                <h4><%= user.email %> </h4>
                <h4><%= user.phoneNo %> </h4>
            <%}); %> 
        </div>
    </div>
    <script></script>
</body>

</html>